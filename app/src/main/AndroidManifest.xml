<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.zhuoxin.feicui.phonesafe">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.GuideActivity">
        </activity>
        <activity android:name=".ui.DemoActivity">
        </activity>
        <activity android:name=".ui.TelClassListActivity">
        </activity>
        <activity android:name=".ui.WelcomeActivity">
        </activity>
        <activity android:name=".ui.SettingActivity">
        </activity>
        <activity android:name=".ui.TelNumberActivity">
        </activity>
        <activity android:name=".ui.PhoneActivity">
        </activity>
        <activity android:name=".ui.SoftManager">
        </activity>
        <activity android:name=".ui.SoftWareActivity">
        </activity>
        <activity android:name=".ui.CleanActivity">
        </activity>
        <activity android:name=".ui.RocketActivity">
        </activity>
        <activity android:name=".ui.FileActivity">
        </activity>

    </application>

</manifest>